{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/components/*": ["./components/*"],
      "@/constants/*": ["./constants/*"],
      "@/enums/*": ["./enums/*"],
      "@/hooks/*": ["./hooks/*"],
      "@/interfaces/*": ["./interfaces/*"],
      "@/lib/*": ["./lib/*"],
      "@/navigation/*": ["./navigation/*"],
      "@/styles/*": ["./styles/*"],
      "@/test/*": ["./test/*"],
      "@/root/*": ["./*"]
    },
    "typeRoots": ["./types"],
    "incremental": true
  },
  "include": ["types/*.d.ts", "**/*.ts", "**/*.tsx", "**/*.js"]
}
